        "module": "CommonJS",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": false,
        "skipLibCheck": true,
        "outDir": "./dist",
        "rootDir": ".."
    },
    "include": [
        "./**/*.ts",
        "../server/**/*.ts"
    ],
    "exclude": [
